{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [".", "./lib", "./lib/util", "./README.md"],
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": false,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
